<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.27.xsd"
        objectQuotingStrategy="QUOTE_ONLY_RESERVED_WORDS">


    <changeSet id="1727686241411-4" author="vanzoneway" context="dev">
        <loadData
                tableName="driver"
                file="db/changelog/data/driver-mock-data.csv"
                separator=","
                encoding="UTF-8"/>
    </changeSet>

    <changeSet id="1727686241411-5" author="vanzoneway" context="dev">
        <loadData
                tableName="car"
                file="db/changelog/data/car-mock-data.csv"
                separator=","
                encoding="UTF-8"/>
    </changeSet>
    <changeSet id="1727686241411-6" author="vanzoneway" context="dev">
        <sql>
            SELECT setval(pg_get_serial_sequence('car', 'id'), coalesce(max(id), 0) + 1, false) FROM car;
        </sql>
    </changeSet>

    <changeSet id="1727686241411-7" author="vanzoneway" context="dev">
        <sql>
            SELECT setval(pg_get_serial_sequence('driver', 'id'), coalesce(max(id), 0) + 1, false) FROM driver;
        </sql>
    </changeSet>
</databaseChangeLog>